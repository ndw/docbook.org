   <define name="db.href.attribute">
      <interleave>
         <attribute name="xlink:href"/>
         <optional>
            <attribute name="xlink:type" dtd:defaultValue="simple">
               <value>simple</value>
            </attribute>
         </optional>
         <optional>
            <attribute name="xlink:role">
               <data type="anyURI"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="xlink:arcrole">
               <data type="anyURI"/>
            </attribute>
         </optional>
         <optional>
            <attribute name="xlink:title"/>
         </optional>
         <optional>
            <attribute name="xlink:show">
               <choice>
                  <value>new</value>
                  <value>replace</value>
                  <value>embed</value>
                  <value>other</value>
               </choice>
            </attribute>
         </optional>
         <optional>
            <attribute name="xlink:actuate">
               <choice>
                  <value>onLoad</value>
                  <value>onRequest</value>
                  <value>other</value>
                  <value>none</value>
               </choice>
            </attribute>
         </optional>
      </interleave>
   </define>

