         <interleave>
            <optional>
               <ref name="db.link.role.attribute"/>
            </optional>
            <ref name="db.common.attributes"/>
            <ref name="db.common.req.linking.attributes"/>
            <optional>
               <attribute name="endterm">
                  <data type="IDREF"/>
               </attribute>
            </optional>
         </interleave>
      </define>
